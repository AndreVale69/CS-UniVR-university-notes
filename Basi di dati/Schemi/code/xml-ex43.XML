<xsd:complexType name="PersonType">
    <xsd:sequence>
        <xsd:element name="Title" type="xsd:string"/>
        <xsd:element name="FirstName" type="xsd:string"/>
        <xsd:element name="Surname" type="xsd:string"/>
    </xsd:sequence>
</xsd:complexType>
<xsd:element name="Person" type="PersonType"/>